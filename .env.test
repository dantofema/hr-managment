# Test environment configuration
APP_ENV=test
APP_SECRET=test_secret_key_for_phpunit_tests_only

# Test database configuration
DATABASE_URL="postgresql://symfony:change-me@127.0.0.1:5432/symfony?serverVersion=15&charset=utf8"

# CORS configuration for tests
CORS_ALLOW_ORIGIN='^https?://(localhost|127\.0\.0\.1)(:[0-9]+)?$'

# JWT configuration for tests
JWT_SECRET_KEY=%kernel.project_dir%/config/jwt/private.pem
JWT_PUBLIC_KEY=%kernel.project_dir%/config/jwt/public.pem
JWT_PASSPHRASE=test_jwt_passphrase
JWT_TTL=3600